<!--pages/summarize/summarize.wxml-->
<view class="container">
  
  <icon type="success" size="70"></icon>
  <view class="over-item">结束</view>
  
  <view class="again" hover-class="active" hover-stay-time="150" bindtap="repeat">再来一组</view>
  <view class="finish" hover-class="active" hover-stay-time="150" bindtap="toastShow">打卡</view> 
  <toast hidden="{{status}}" duration="3000" bindchange="toastHide">打卡成功</toast>
</view>
